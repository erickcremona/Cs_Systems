<?xml version="1.0" encoding="ISO-8859-1"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:simpleType name="DataFormatoBrasil">
		<xsd:restriction base="xsd:string">
			<xsd:length value="10"/>
			<xsd:pattern value="(((0[1-9]|[1,2][0-9])/02)|((0[1-9]|[1,2][0-9]|30)/(0[4,6,9]|11))|((0[1-9]|[1,2][0-9]|3[0,1])/(0[1,3,5,7,8]|1[0,2])))/[1,2][0-9]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="DataHora">
	<xsd:restriction base="xsd:string">
		<xsd:length value="19"/>
		<xsd:pattern value="((0[1-9])|([12][0-9])|(3[01]))/((0[1-9])|(1[012]))/((000[1-9])|(00[1-9][0-9])|(0[1-9][0-9]{2})|([1-9][0-9]{3})) (([01][0-9])|(2[0-3]))(:[0-5][0-9]){2}(\.[0-9]+)?" /> 
	</xsd:restriction>
</xsd:simpleType>
	
	<xsd:simpleType name="FormatoSeloOriginal">
		<xsd:restriction base="xsd:string">
		<xsd:minLength value="8"/>
		<xsd:maxLength value="9"/>
			<xsd:pattern value="^[A-Z]{3,4}[0-9]{5}$"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="FormatoSelo">
		<xsd:restriction base="xsd:string">
			<xsd:length value="9"/>
			<xsd:pattern value="^[A-Z]{4}[0-9]{5}$"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="FormatoAleatorio">
		<xsd:restriction base="xsd:string">
			<xsd:length value="3"/>
			<xsd:pattern value="^[A-Z]{3}$"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="CodigoServico">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
			<xsd:pattern value="^[0-9]{1,4}$"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<!-- Define o tipo complexo  -->
	
	<xsd:complexType name="Ato" abstract="true">
	<xsd:attribute name="DataPratica" use="required">
		<xsd:simpleType>
		<xsd:restriction base="DataFormatoBrasil"/>
		</xsd:simpleType>
	</xsd:attribute>
	<xsd:attribute name="Selo" use="required">
		<xsd:simpleType>
		<xsd:restriction base="FormatoSelo"/>
		</xsd:simpleType>
	</xsd:attribute>
	<xsd:attribute name="Aleatorio" use="required">
		<xsd:simpleType>
		<xsd:restriction base="FormatoAleatorio"/>
		</xsd:simpleType>
	</xsd:attribute>					
	<xsd:attribute name="Livro">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:minLength value="2"/>
				<xsd:maxLength value="25"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:attribute>
	<xsd:attribute name="FolhaInicial">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:minLength value="2"/>
				<xsd:maxLength value="25"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:attribute>
	<xsd:attribute name="FolhaFinal">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:minLength value="2"/>
				<xsd:maxLength value="25"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:attribute>
	<xsd:attribute name="NumeroAto">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:minLength value="2"/>
				<xsd:maxLength value="25"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:attribute>
	<xsd:attribute name="LocalPratica">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="S"/>
				<xsd:enumeration value="F"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="Transmissao" abstract="true">
		<xsd:attribute name="DataPratica" use="required">
			<xsd:simpleType>
				<xsd:restriction base="DataFormatoBrasil"/>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="CCT" use="required">
			<xsd:simpleType>
				<xsd:restriction base="FormatoSelo"/>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	
	<xsd:complexType name="ElementoJustificativaAtoNaoPraticado">
		<xsd:attribute name="Data" use="required">
			<xsd:simpleType>
				<xsd:restriction base="DataFormatoBrasil"/>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	
	<!--TIPOS DE ATOS-->

	<xsd:complexType name="AtoEscritura">
	<xsd:complexContent> 
		<xsd:extension base="Ato">
			<xsd:sequence> 
				<xsd:element name="DadosProcuracao" type="tpDadosProcuracao" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Objetos" type="tpObjetos" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Participantes" type="tpParticipantes" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="AtosVinculados" type="tpAtosVinculados" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="TipoAto" use="required">   
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="NLE"/>
						<xsd:enumeration value="NCE"/>						
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="TipoEscritura" use="required">   
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="3"/>
						<xsd:pattern value="^[0-9]{1,3}$"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
					<xsd:attribute name="Descricao">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
							<xsd:minLength value="2"/>
							<xsd:maxLength value="250"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="IndAlvaraJudicial" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="S"/>
						<xsd:enumeration value="N"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="IndPorProcuracao" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="S"/>
						<xsd:enumeration value="N"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="IndValorDeclarado" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="S"/>
						<xsd:enumeration value="N"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="ValorDeclarado">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2"/>
						<xsd:minInclusive value="0"/>
						<xsd:totalDigits value="12"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
					<xsd:attribute name="Moeda">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="Cr$"/>
						<xsd:enumeration value="NCr$"/>
						<xsd:enumeration value="Cz$"/>
						<xsd:enumeration value="NCz$"/>
						<xsd:enumeration value="CR$"/>
						<xsd:enumeration value="R$"/>
					</xsd:restriction>
				</xsd:simpleType>
				</xsd:attribute>	    			
		</xsd:extension>
	</xsd:complexContent> 
	</xsd:complexType>
	
	<xsd:complexType name="AtoProcuracao">
	<xsd:complexContent> 
		<xsd:extension base="Ato">
			<xsd:sequence>
				<xsd:element name="ProcuracaoOriginal" type="tpProcuracaoOriginal" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Participantes" type="tpParticipantes" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="AtosVinculados" type="tpAtosVinculados" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence> 
			<xsd:attribute name="TipoAto" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
								<xsd:enumeration value="NLP"/>
								<xsd:enumeration value="NCP"/>
								<xsd:enumeration value="NSP"/>
								<xsd:enumeration value="NRP"/>				        				        
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="FinalidadeProcuracao">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="TipoLivro" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
							<xsd:enumeration value="P"/>
							<xsd:enumeration value="E"/>
							<xsd:enumeration value="M"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="DataVigencia">
				<xsd:simpleType>
					<xsd:restriction base="DataFormatoBrasil"/>
				</xsd:simpleType>
			</xsd:attribute>			
		</xsd:extension>
	</xsd:complexContent> 
	</xsd:complexType>
	
	<xsd:complexType name="AtoTestamento">
	<xsd:complexContent> 
		<xsd:extension base="Ato">
			<xsd:sequence>
				<xsd:element name="TestamentoOriginal" type="tpTestamentoOriginal" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Testador" type="tpPessoaFisica" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="AtosVinculados" type="tpAtosVinculados" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence> 
			<xsd:attribute name="TipoAto" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
								<xsd:enumeration value="NRTP"/>
								<xsd:enumeration value="NRTC"/>
						<xsd:enumeration value="NCTP"/>
								<xsd:enumeration value="NCTC"/>				        				        
						<xsd:enumeration value="NTR"/>
						<xsd:enumeration value="NCTR"/>
				 </xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>	  	    			
		</xsd:extension>
	</xsd:complexContent> 
	</xsd:complexType>
	
	<xsd:complexType name="AtoFirma">
	<xsd:complexContent> 
		<xsd:extension base="Ato">
			<xsd:sequence>
				<xsd:element name="PessoaFirma" type="tpPessoaFisica" minOccurs="1" maxOccurs="unbounded"/>
				<xsd:element name="AtosVinculados" type="tpAtosVinculados" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence> 
			<xsd:attribute name="TipoAto" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
								<xsd:enumeration value="NAF"/>
								<xsd:enumeration value="NRFS"/>
								<xsd:enumeration value="NRFA"/>
								<xsd:enumeration value="NRFC"/>				        				        
								<xsd:enumeration value="NRFP"/>
						</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>	  	    			
		</xsd:extension>
	</xsd:complexContent> 
	</xsd:complexType>
	
	<xsd:complexType name="AtoAutenticacao">
	<xsd:complexContent> 
		<xsd:extension base="Ato" >
		<xsd:sequence>
			<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
		</xsd:sequence>
		</xsd:extension>
	</xsd:complexContent> 
	</xsd:complexType>
	
	<xsd:complexType name="AtoCertidaoGenerica">
	<xsd:complexContent> 
		<xsd:extension base="Ato">
			<xsd:sequence>
				<xsd:element name="Participantes" type="tpParticipantes" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="AtosVinculados" type="tpAtosVinculados" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence> 
			<xsd:attribute name="TipoCertidao" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
							<xsd:enumeration value="G1"/>
							<xsd:enumeration value="G2"/>
							<xsd:enumeration value="G3"/>
							<xsd:enumeration value="G4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="IndNegativa" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						 <xsd:enumeration value="S"/>
						 <xsd:enumeration value="N"/>							        				        						
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:extension>
	</xsd:complexContent> 
	</xsd:complexType>  
	
	<xsd:complexType name="AtoMaterializacaoDocEletronico">
	<xsd:complexContent> 
		<xsd:extension base="Ato">
			<xsd:sequence>				
							<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="OrigemDoc" use="required">   
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="W"/>
						<xsd:enumeration value="A"/>						
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="QtdTotalFolhas" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="6"/>
								<xsd:pattern value="^[0-9]{1,6}$"/>
							</xsd:restriction>
						</xsd:simpleType>
						</xsd:attribute>
			<xsd:attribute name="NumeroFolha" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="6"/>
								<xsd:pattern value="^[0-9]{1,6}$"/>
							</xsd:restriction>
						</xsd:simpleType>
						</xsd:attribute>
			<xsd:attribute name="DataImpressao" use="required">
						<xsd:simpleType>
						<xsd:restriction base="DataHora"/>
						</xsd:simpleType>
				</xsd:attribute>
			<xsd:attribute name="URL">   
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="2"/>
						<xsd:maxLength value="1000"/>						
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>		    
			<xsd:attribute name="NomeArquivo">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
							<xsd:minLength value="2"/>
						<xsd:maxLength value="100"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
					<xsd:attribute name="FormatoArquivo">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
							<xsd:minLength value="2"/>
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="DataArquivo">
						<xsd:simpleType>
						<xsd:restriction base="DataHora"/>
						</xsd:simpleType>
				</xsd:attribute>
			<xsd:attribute name="TamanhoArquivo">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"/>
						<xsd:maxLength value="10"/>
						<xsd:pattern value="^[0-9]{1,10}$"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>				    			
		</xsd:extension>
	</xsd:complexContent> 
	</xsd:complexType>
	
	<xsd:complexType name="AtoVisto">
	<xsd:complexContent> 
		<xsd:extension base="Ato">
			<xsd:sequence>
				<xsd:element name="Participantes" type="tpParticipantes" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="AtosVinculados" type="tpAtosVinculados" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="Revalidada" use="required">
					<xsd:simpleType>
					<xsd:restriction base="xsd:string">
							<xsd:enumeration value="S"/>
							<xsd:enumeration value="N"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:extension>
	</xsd:complexContent> 
	</xsd:complexType>   

	<xsd:complexType name="tpFolhaAdicionalComplemento">
		<xsd:complexContent>
			<xsd:extension base="Transmissao">
				<xsd:sequence>
					<xsd:element name="AtosVinculados" type="tpAtosVinculados" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
				</xsd:sequence>
				<xsd:attribute name="QtdPaginasExcedentes" use="required">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"/>
							<xsd:maxLength value="6"/>
							<xsd:pattern value="^[0-9]{1,6}$"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
	 </xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="tpInformacaoVerbal">
		<xsd:complexContent>
			<xsd:extension base="Transmissao">
				<xsd:sequence>
					<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
				</xsd:sequence>
				<xsd:attribute name="NumeroAto" use="required">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="2"/>
							<xsd:maxLength value="25"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="Nome" use="required">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="2"/>
							<xsd:maxLength value="200"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AtoComplementoEmolumentos">
		<xsd:complexContent>
			<xsd:extension base="Transmissao">
				<xsd:sequence>
					<xsd:element name="AtosVinculados" type="tpAtosVinculados" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="AtoAlteracaoEmolumentos">
				<xsd:sequence>
					 <xsd:element name="Emolumentos" type="tpEmolumentos" minOccurs="1" maxOccurs="1"/>         
				</xsd:sequence>
		<xsd:attribute name="Selo" use="required">
				<xsd:simpleType>
				<xsd:restriction base="FormatoSelo"/>
				</xsd:simpleType>
			</xsd:attribute>	
	</xsd:complexType>
	
	<!--Elementos-->

	<xsd:complexType name="tpProcuracaoOriginal">
		<xsd:attribute name="LivroProcOriginal" >
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="2"/>
					<xsd:maxLength value="25"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
			<xsd:attribute name="FolhaInicialProcOriginal" >
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="2"/>
						<xsd:maxLength value="25"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		<xsd:attribute name="FolhaFinalProcOriginal" >
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="2"/>
					<xsd:maxLength value="25"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="NumeroAtoProcOriginal" >
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="2"/>
					<xsd:maxLength value="25"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="IndLavradoRio" >
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="S"/>
					<xsd:enumeration value="N"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="ServicoProcOriginal">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="2"/>
					<xsd:maxLength value="250"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="EstadoProcOriginal">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length fixed="true" value="2"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="DataProcOriginal">
			<xsd:simpleType>
				<xsd:restriction base="DataFormatoBrasil"/>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="TipoLivroProcOriginal" >
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="P"/>
					<xsd:enumeration value="E"/>
					<xsd:enumeration value="M"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="IndOutorgadoAusente" >
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="S"/>
					<xsd:enumeration value="N"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="NumeroNotificacao" >
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="2"/>
					<xsd:maxLength value="25"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>


	<xsd:complexType name="tpTestamentoOriginal">
		<xsd:attribute name="LivroTestOriginal" >
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="2"/>
					<xsd:maxLength value="25"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
